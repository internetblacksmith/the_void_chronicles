{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cd ssh-reader && go build -o void-reader ."
  },
  "deploy": {
    "startCommand": "cd ssh-reader && ./void-reader",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}